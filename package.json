{
  "name": "react-image-closeup",
  "version": "1.0.0",
  "description": "Display an image in a full screen modal",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testing mocha './test/*' --watch --require babel-core/register",
    "start": "NODE_ENV=production npm run build && npm run server",
    "dev:server": "webpack-serve ./webpack.config.js",
    "dev:start": "concurrently \"npm:dev:build\" \"npm:dev:server\"",
    "lint": "eslint src/**",
    "build": "node buildCss.js && babel src -d dist",
    "dev:build": "chokidar 'src/*.js*' 'src/*.css' -c 'npm run build'"
  },
  "author": "grittly",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.6.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "postcss": "^7.0.2",
    "postcss-cli": "^6.0.0",
    "postcss-import": "^12.0.0",
    "postcss-modules": "^1.3.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2"
  }
}
